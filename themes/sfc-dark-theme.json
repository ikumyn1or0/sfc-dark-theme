{
	"name": "sfc-dark-theme",
	"type": "dark",
	"colors": {
		"foreground": "#b9bbed", //文字色
		// "panelTitle.inactiveForeground": "#ff0000",
		//エディタ
		"editor.foreground": "#b9bbed", //文字色
		"editor.background": "#080821", //背景
		"editor.lineHighlightBackground": "#2e1f7a33", //行ハイライト背景
		"editorCursor.foreground": "#a5750d", //カーソル色
		"editorGutter.background": "#12125433", //ガーター背景
		"editorWhitespace.foreground": "#b9bbed44", //空白文字色
		"editorLineNumber.foreground": "#b9bbed88", //行番号文字色
		"editorLineNumber.activeForeground": "#a5750d88", //アクティブ時行番号文字色
		"editorBracketHighlight.foreground1": "#cea549", //ブラケット1
		"editorBracketHighlight.foreground2": "#b76ada", //ブラケット2
		"editorBracketHighlight.foreground3": "#127eff", //ブラケット3
		"editorLink.activeForeground": "#3f80f8", //リンクアクティブ時文字色
		//エディタグループ
		"editorGroup.border": "#121254", //枠線
		"editorGroup.dropBackground": "#251962aa", //タブドロップバック時背景
		"editorGroupHeader.tabsBackground": "#080821", //タブ背景
		//アクティビティバー
		"activityBar.background": "#121254", //背景
		"activityBar.activeBorder": "#a5750d", //アクティブ時枠線
		"activityBarBadge.background": "#a5750d", //背景
		"activityBar.inactiveForeground": "#b9bbed", //非アクティブ時文字色
		//タイトルバー
		"titleBar.activeBackground": "#121254", //アクティブ時背景
		"titleBar.activeForeground": "#b9bbed", //アクティブ時前景
		//サイドバー
		"sideBar.background": "#0b0b32", //背景
		"sideBarSectionHeader.background": "#121254", //セクション背景
		//タブ
		"tab.border": "#121254", //枠線
		"tab.activeBackground": "#121254", //アクティブ時背景
		"tab.selectedBorderTop": "#a5750d", //選択時枠線
		"tab.inactiveBackground": "#0b0b32", //非アクティブ時背景
		"tab.inactiveForeground": "#b9bbed",
		//エディタウィジェット
		"editorWidget.background": "#0b0b32", //背景
		"editorWidget.border": "#121254", //枠線
		//パネル(ターミナル)
		"panel.border": "#121254", //枠線
		"panelTitle.inactiveForeground": "#b9bbed", //非アクティブ時文字色
		//入力ウィジェット
		"input.background": "#17173c", //背景
		"inputOption.hoverBackground": "#2e1f7a", //ホバー時背景
		"inputOption.activeBackground": "#2e1f7a", //選択時背景
		"inputOption.activeBorder": "#a5750d", //選択時枠線
		//ドロップダウンウィジェット
		"dropdown.background": "#17173c", //背景
		//ステータスバー
		"statusBar.background": "#51328f", //背景
		"statusBar.debuggingBackground": "#51328f", //デバッグ時背景
		"statusBar.noFolderBackground": "#51328f", //フォルダなし時背景
		"statusBarItem.prominentBackground": "#a5750d", //アイテム背景
		"statusBarItem.remoteBackground": "#a5750d", //リモート背景
		//ボタン
		"button.background": "#51328f", //背景
		//プログレスバー
		"progressBar.background": "#51328f", //背景
		//バッジ
		"badge.background": "#a5750d", //背景
		//フォーカスアイテム
		"focusBorder": "#a5750d", //枠線
		//リスト
		"list.activeSelectionBackground": "#2e1f7a", //選択アイテム背景
		"list.inactiveSelectionBackground": "#251962", //選択アイテム非アクティブ時背景
		"list.focusBackground": "#2e1f7a", //フォーカスアイテム背景
		"list.hoverBackground": "#2e1f7a", //ホバーアイテム背景
		"list.dropBackground": "#251962", //ドロップバック時背景
		//スクロールバー
		"scrollbar.shadow": "#1f1fad", //影
		"scrollbarSlider.background": "#1f1fad44", //背景
		"scrollbarSlider.hoverBackground": "#1f1fad77", //ホバー時背景
		"scrollbarSlider.activeBackground": "#1f1fadaa", //アクティブ時背景
		//
		//
		//
		//未整理
		// //文字
		// //選択文字
		"selection.background": "#f2aa0d33",
		"editor.selectionBackground": "#f2aa0d33",
		"editor.findMatchHighlightBackground": "#f2aa0d33",
		"editor.wordHighlightBackground": "#f2aa0d33",
		"editor.wordHighlightStrongBackground": "#f2aa0d33",
		"editor.wordHighlightTextBackground": "#f2aa0d33",
		"editor.inactiveSelectionBackground": "#f2aa0d33",
		"minimap.findMatchHighlight": "#f2aa0d33",
		"minimap.selectionHighlight": "#f2aa0d33",
		"editor.selectionHighLightBackground": "#f2aa0d33"
		// //その他
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python",
				"meta.template.expression",
				"keyword.operator",
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#b9bbed"
			}
		},
		{
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.quote.begin.markdown"
			],
			"settings": {
				"foreground": "#678a86"
			}
		},
		{
			"scope": [
				"constant.language",
				"entity.name.tag",
				"markup.changed",
				"meta.preprocessor",
				"entity.name.function.preprocessor",
				"meta.diff.header",
				"storage",
				"storage.type",
				"storage.modifier",
				"keyword.operator.noexcept",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded",
				"keyword",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike",
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php",
				"variable.language",
				"punctuation.definition.list.begin.markdown",
				"token.info-token"
			],
			"settings": {
				"foreground": "#5e90e3"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.bold"
			],
			"settings": {
				"foreground": "#5e90e3",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent",
				"markup.inserted",
				"meta.preprocessor.numeric",
				"keyword.other.unit",
				"constant.sha.git-rebase"
			],
			"settings": {
				"foreground": "#9fadd6"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#4347b8"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss",
				"entity.name.tag.css",
				"entity.name.tag.less",
				"token.warn-token"
			],
			"settings": {
				"foreground": "#b5a2a2"
			}
		},
		{
			"scope": [
				"invalid",
				"token.error-token",
				"string.regexp"
			],
			"settings": {
				"foreground": "#d26a8f"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"markup.inline.raw",
				"meta.preprocessor.string",
				"string",
				"meta.embedded.assembly",
				"string.tag",
				"string.value"
			],
			"settings": {
				"foreground": "#be8a8a"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded",
				"meta.structure.dictionary.key.python",
				"support.function.git-rebase",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#9ab9ff"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#a485f4"
			}
		}
	]
}
